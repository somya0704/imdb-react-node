{
    "extends": "eslint-config-airbnb",
    "plugins": [
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
           
        },
        "env": {
            "browser": true,
            "node": true,
            "es6": true
        }
    },
    "rules": {
        "import/prefer-default-export": 1,
        "no-shadow": 1,
        "consistent-return": 1,
        "no-underscore-dangle" : 0,
        "one-var" : 1,
        "prefer-const": 1
      }
}
